{
  "name": "algorithm",
  "version": "1.0.0",
  "description": "Simple ways to understand some of the basic algorithms.",
  "main": "nofile.js",
  "scripts": {
    "no": "no",
    "test": "no lint test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ysmood/algorithm.git"
  },
  "keywords": [
    "algorithm"
  ],
  "author": "http://ysmood.org",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ysmood/algorithm/issues"
  },
  "homepage": "https://github.com/ysmood/algorithm#readme",
  "devDependencies": {
    "eslint": "2.11.1",
    "junit": "1.4.4",
    "nokit": "0.20.12"
  },
  "eslintConfig": {
    "rules": {
      "indent": [
        2,
        4
      ],
      "linebreak-style": [
        2,
        "unix"
      ],
      "semi": [
        2,
        "always"
      ],
      "no-trailing-spaces": 2,
      "space-before-function-paren": [
        2,
        "always"
      ],
      "eqeqeq": [
        2,
        "allow-null"
      ],
      "no-unused-vars": 0,
      "no-console": [
        0
      ]
    },
    "env": {
      "browser": true,
      "node": true
    },
    "extends": "eslint:recommended"
  }

}
